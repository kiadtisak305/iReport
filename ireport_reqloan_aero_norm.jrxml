	<queryString>
		<![CDATA[select ft_getmbname(mb.coop_id , mb.member_no) as fullname , mb.birth_date, lq.loanrequest_date as loanrequest_date,lq.paymonth_exp,lq.paymonth_other,lq.expense_code,lq.expense_accid, lq.loanrcvfix_date,lq.loantype_code,mb.mem_telmobile,lq.paymonth_coop,lq.paymonth_lnreq,
lq.member_no  ,mb.membgroup_code , mg.membgroup_desc , mb.mem_tel ,mb.member_date,
lq.salary_amt as salary_amount , lq.share_balance , lq.member_age ,lq.salary_perc, lq.recvestimate_amt,lt.prefix,lq.incomemonth_other,lq.salary_70,lq.salary_bal,
lt.loantype_desc ,  lq.loanrequest_docno , lq.loanrequest_amt, lq.loanreqregis_amt,
lq.loanpermiss_amt, lq.loanrequest_amt, FT_CALAGE(birth_date, lq.loanrequest_date,4) as age,
lq.bfretire_period , lq.bfretire_payment , lq.afretire_period , lq.afretire_payment,
FTCM_CALAGEMTH(member_date, lq.loanrequest_date) as member_ages, lq.incomemonth_fix, lq.dept_amt,lq.remark,lq.loancredit_amt,lq.share_lastperiod,trunc(lq.share_lastperiod/12) as mbyear,mod(lq.share_lastperiod,12) as mbmonth,
 lq.period_payment , lq.period_payamt,
lq.intestimate_amt , lq.period_lastpayment , lq.intestimate_amt,lq.share_periodvalue,lq.paymonth_exp , lq.period_payment ,  lq.recvestimate_amt,lq.intestimate_amt , lq.salary_amt, lt.loantype_desc,cbb.bank_desc,cmm.moneytype_desc,
nvl((select  count(lnexp.moneytype_code ) from lnreqloanpayoutexpense lnexp where lnexp.loanrequest_docno = lq.loanrequest_docno) ,0) as count_lnexp,
(select oth.clrother_amt from lnreqloanclrother oth where lq.loanrequest_docno = oth.loanrequest_docno and oth.clrothertype_code = 'SHD') as shr_buy,
(select oth.clrother_amt from lnreqloanclrother oth where lq.loanrequest_docno = oth.loanrequest_docno and oth.clrothertype_code != 'SHD') as clroth_amt,
( select sum(clr.principal_balance) from lnreqloanclr clr where lq.loanrequest_docno = clr.loanrequest_docno ) as sumloan_balance,
( select sum(clr1.principal_balance + clr1.intreal_amt) from lnreqloanclr clr1 where lq.loanrequest_docno = clr1.loanrequest_docno and clr1.clear_status = 1 ) as sumloanclr_balance,
am.full_name as entry_fullname, am.description, (lq.incomemonth_fix + lq.incomemonth_other) as incomemonth,  sh.payment_status,
nvl((select (sum(period_payment) + sum(intestimate_amt)) as sum_amt from lnreqloanclr where loanrequest_docno = lq.loanrequest_docno and clear_status = 0 group by loanrequest_docno), 0) a,
nvl((select count(lns.loanitemtype_code) from lncontstatement lns join lncontmaster lnm on lns.loancontract_no = lnm.loancontract_no join lnreqloan lq2 on lnm.member_no = lq2.member_no
where lq2.loanrequest_docno = lq.loanrequest_docno and lns.loanitemtype_code = 'RPM ' and lns.slip_date between ADD_MONTHS(TRUNC(SYSDATE, 'MONTH'), -24) and sysdate), 0) chln
from lnreqloan lq
left join mbmembmaster mb on lq.coop_id = mb.coop_id and lq.member_no = mb.member_no
left join mbucfmembgroup mg on mb.coop_id = mg.coop_id and mb.membgroup_code = mg.membgroup_code
join lnloantype lt on lq.coop_id = lt.coop_id and lq.loantype_code = lt.loantype_code
left join amsecusers am on lq.entry_id = am.user_name
left join shsharemaster sh on mb.member_no = sh.member_no
left join lnreqloanclrother clr on lq.loanrequest_docno = clr.loanrequest_docno
left join cmucfmoneytype cmm on lq.expense_code = cmm.moneytype_code
left join cmucfbank cbb on lq.expense_bank = cbb.bank_code
where
$X{IN, ltrim(rtrim(lq.loanrequest_docno)) , list_docno}
and lq.loanrequest_status <> -9
order by lq.loanrequest_docno]]>
	</queryString>
